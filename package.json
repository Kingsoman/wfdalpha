{
    "name": "wefundapp",
    "private": false,
    "license": "MIT",
    "scripts": {
        "start": "react-static start",
        "stage": "react-static build --staging",
        "build": "react-static build",
        "analyze": "react-static build --analyze",
        "serve": "serve dist -p 3000",
        "lint": "prettier --write .",
        "sync-web-server": "rsync -azP --delete --exclude 'media/' dist/ deploy@wefund.app:~/wefund.app",
        "sync-cdn": "s3cmd sync public/media/* --delete-removed --acl-public s3://wfdmedia/",
        "deploy": "yarn build && yarn sync-cdn && yarn sync-web-server"
    },
    "dependencies": {
